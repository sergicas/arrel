{
    "appId": "eu.arrel.app",
    "appName": "Arrel",
    "webDir": "dist",
    "server": {
        "androidScheme": "https",
        "iosScheme": "https"
    },
    "ios": {
        "contentInset": "automatic",
        "backgroundColor": "#050505",
        "preferredContentMode": "mobile"
    },
    "plugins": {
        "SplashScreen": {
            "launchShowDuration": 2000,
            "launchAutoHide": true,
            "backgroundColor": "#050505",
            "androidSplashResourceName": "splash",
            "androidScaleType": "CENTER_CROP",
            "showSpinner": false,
            "iosSpinnerStyle": "small",
            "spinnerColor": "#999999"
        },
        "StatusBar": {
            "style": "dark",
            "backgroundColor": "#050505"
        }
    }
}